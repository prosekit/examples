{
  "name": "prosekit-examples",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "echo 'noop'",
    "clone": "bash .scripts/clone.sh",
    "copy": "bun .scripts/build.ts",
    "fix": "prettier --write ."
  },
  "dependencies": {
    "@types/bun": "1.1.13",
    "@types/node": "22.9.1",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.4"
  },
  "renovate": {
    "dependencyDashboard": true,
    "branchNameStrict": true,
    "rangeStrategy": "bump",
    "automerge": true,
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)",
      ":disablePeerDependencies",
      ":includeNodeModules"
    ]
  }
}
