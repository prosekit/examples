{
  "name": "prosekit-examples",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "echo 'noop'",
    "clone": "bash .scripts/clone.sh",
    "copy": "bun .scripts/build.ts",
    "fix": "prettier --write ."
  },
  "dependencies": {
    "@types/bun": "1.1.12",
    "@types/node": "20.16.15",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3"
  },
  "renovate": {
    "dependencyDashboard": true,
    "branchNameStrict": true,
    "rangeStrategy": "bump",
    "automerge": true,
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)",
      ":disablePeerDependencies",
      ":includeNodeModules"
    ]
  }
}
