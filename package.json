{
  "name": "prosekit-examples",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "echo 'No build step'",
    "fix": "prettier --write ."
  },
  "dependencies": {
    "prettier": "^3.2.4"
  },
  "renovate": {
    "dependencyDashboard": true,
    "branchNameStrict": true,
    "rangeStrategy": "bump",
    "extends": [
      "config:base",
      ":semanticCommitTypeAll(chore)",
      ":disablePeerDependencies",
      ":includeNodeModules"
    ],
    "packageRules": [
      {
        "groupName": "prosekit",
        "matchPackagePatterns": [
          "prosekit"
        ]
      },
      {
        "groupName": "dependencies",
        "excludePackagePatterns": [
          "prosekit"
        ],
        "extends": [
          "schedule:monthly"
        ]
      }
    ]
  }
}
